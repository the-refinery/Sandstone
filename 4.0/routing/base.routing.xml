<routing>

	<default>
		<url></url>
		<page>home</page>
	</default>

	<application>
		<url>application</url>
		<page></page>
	</application>

	<directpagereference>
		<url></url>
		<page></page>
	</directpagereference>

	<login>
		<url>login</url>
		<page>login</page>
	</login>

	<javascript>
		<url>javascript/{library}</url>
		<page>javascript</page>
	</javascript>

	<resource>
		<url>resource</url>
		<page>resource</page>
	</resource>

	<KnowledgebaseArticle>
		<url>knowledgebase/{seopagename}</url>
		<page>knowledgebaseview</page>
		<class>knowledgebasearticle</class>
		<action>view</action>
	</KnowledgebaseArticle>

	<searchresults>
		<url>searchresults</url>
		<page>searchresults</page>
		<class>search</class>
		<action>view</action>
	</searchresults>

</routing>